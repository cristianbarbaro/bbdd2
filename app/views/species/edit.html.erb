<h1>Editing Species</h1>

<%= render 'form', species: @species %>

<div class="pull-right">
    <%= link_to 'Delete', @species, class:'btn btn-danger', method: :delete, data: { confirm: 'Are you sure?' }, title: 'Delete' %>
    <%= link_to 'Show', @species, class: "btn btn-primary" %>
    <%= link_to 'Back', :back, class: "btn btn-default" %>
</div>