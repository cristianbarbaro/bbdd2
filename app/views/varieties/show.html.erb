<p>
  <strong>Name:</strong>
  <%= @variety.name %>
</p>

<p>
  <strong>Scientific name:</strong>
  <%= @variety.scientific_name %>
</p>

<p>
  <strong>Url:</strong>
  <%= @variety.url %>
</p>

<p>
  <strong>Description:</strong>
  <%= @variety.description %>
</p>

<div class="pull-right">
  <%= link_to 'Edit', edit_variety_path(@variety), class: "btn btn-success" %>
  <%= link_to 'Back', varieties_path, class: "btn btn-default" %>
</div>
